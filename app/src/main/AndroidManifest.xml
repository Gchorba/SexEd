<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sexed.appdoctor"
    android:versionCode="3"
    android:versionName="2.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AppDoctor"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.appszoom.appszoomsdk.AppsZoomInterstitialActivity"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name=".Symptoms"
            android:label="@string/title_activity_symptoms"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Diseases"
            android:label="@string/title_activity_diseases"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Medicines"
            android:label="@string/title_activity_medicines"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AboutMe"
            android:label="@string/title_activity_about_me"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SubPainkiller"
            android:label="@string/title_activity_sub_painkiller"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SubAntibiotic"
            android:label="@string/title_activity_sub_antibiotic"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SubAntihistamine"
            android:label="@string/title_activity_sub_antihistamine"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InspirationalQuotes"
            android:label="@string/title_activity_inspirational_quotes"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AccountNo"
            android:label="@string/title_activity_account_no"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResultEvaluate"
            android:label="@string/title_activity_result_evaluate"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DiseaseExplanation"
            android:label="@string/title_activity_disease_explanation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SubAntiinflamatory"
            android:label="@string/title_activity_sub_antiinflamatory" />
        <activity
            android:name=".AlarmListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AlarmScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AlarmDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SetNotification"
            android:label="@string/title_activity_set_notification"
            android:screenOrientation="portrait" />

        <service
            android:name=".AlarmService"
            android:enabled="true"
            android:screenOrientation="portrait" />

        <receiver
            android:name=".AlarmManagerHelper"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <!--
             Detect app installs. Necessary to control the installs your app
            generates (avoid using it if you are only tracking)
        -->
        <receiver android:name="com.appszoom.appszoomsdk.AppsZoomPackageAddedReceiver" >
            <intent-filter>
                <data android:scheme="package" />

                <action android:name="android.intent.action.PACKAGE_ADDED" />
            </intent-filter>
        </receiver>

        <!-- Detect the installation of your app and the source. -->
        <receiver
            android:name="com.appszoom.appszoomsdk.AppsZoomReferrerReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".Education"
            android:label="@string/title_activity_education" >
        </activity>
        <activity
            android:name=".Tools"
            android:label="@string/title_activity_tools" >
        </activity>
        <activity
            android:name=".About"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name=".PeriodTracker"
            android:label="@string/title_activity_period_tracker" >
        </activity>
        <activity
            android:name=".DiseaseIdentifier"
            android:label="@string/title_activity_disease_identifier" >
        </activity>
        <activity
            android:name=".Clinics"
            android:label="@string/title_activity_clinics" >
        </activity>
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".Messages"
            android:label="@string/title_activity_messages" >
        </activity>
    </application>

</manifest>
