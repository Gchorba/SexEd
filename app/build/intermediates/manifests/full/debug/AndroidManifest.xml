<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.medworld.appdoctor"
    android:versionCode="3"
    android:versionName="2.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sexed.appdoctor.AppDoctor"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.appszoom.appszoomsdk.AppsZoomInterstitialActivity"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name="com.sexed.appdoctor.Symptoms"
            android:label="@string/title_activity_symptoms"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.Diseases"
            android:label="@string/title_activity_diseases"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.Medicines"
            android:label="@string/title_activity_medicines"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.AboutMe"
            android:label="@string/title_activity_about_me"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.SubPainkiller"
            android:label="@string/title_activity_sub_painkiller"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.SubAntibiotic"
            android:label="@string/title_activity_sub_antibiotic"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.SubAntihistamine"
            android:label="@string/title_activity_sub_antihistamine"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.InspirationalQuotes"
            android:label="@string/title_activity_inspirational_quotes"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.AccountNo"
            android:label="@string/title_activity_account_no"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.ResultEvaluate"
            android:label="@string/title_activity_result_evaluate"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.DiseaseExplanation"
            android:label="@string/title_activity_disease_explanation"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.SubAntiinflamatory"
            android:label="@string/title_activity_sub_antiinflamatory" />
        <activity
            android:name="com.sexed.appdoctor.AlarmListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.AlarmScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.AlarmDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sexed.appdoctor.SetNotification"
            android:label="@string/title_activity_set_notification"
            android:screenOrientation="portrait" />

        <service
            android:name="com.sexed.appdoctor.AlarmService"
            android:enabled="true"
            android:screenOrientation="portrait" />

        <receiver
            android:name="com.sexed.appdoctor.AlarmManagerHelper"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <!--
             Detect app installs. Necessary to control the installs your app
            generates (avoid using it if you are only tracking)
        -->
        <receiver android:name="com.appszoom.appszoomsdk.AppsZoomPackageAddedReceiver" >
            <intent-filter>
                <data android:scheme="package" />

                <action android:name="android.intent.action.PACKAGE_ADDED" />
            </intent-filter>
        </receiver>

        <!-- Detect the installation of your app and the source. -->
        <receiver
            android:name="com.appszoom.appszoomsdk.AppsZoomReferrerReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.sexed.appdoctor.Education"
            android:label="@string/title_activity_education" >
        </activity>
        <activity
            android:name="com.sexed.appdoctor.Tools"
            android:label="@string/title_activity_tools" >
        </activity>
        <activity
            android:name="com.sexed.appdoctor.About"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.sexed.appdoctor.PeriodTracker"
            android:label="@string/title_activity_period_tracker" >
        </activity>
    </application>

</manifest>